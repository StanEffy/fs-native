{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],

    "setupFilesAfterEnv": ["<rootDir>/setupTests.js"]
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@expo/metro-config": "^0.4.0",
    "@expo/webpack-config": "^0.17.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "apollo-link-context": "^1.0.20",
    "dotenv": "^16.0.3",
    "expo": "~46.0.16",
    "expo-constants": "^13.2.4",
    "expo-font": "~10.2.0",
    "expo-status-bar": "~1.4.0",
    "formik": "^2.2.9",
    "graphql": "^15.8.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-dotenv": "^3.3.1",
    "react-native-web": "~0.18.7",
    "react-router-native": "^6.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^11.3.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.25.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^27.5.1",
    "jest-expo": "^46.0.1",
    "react-test-renderer": "18.0.0"
  },
  "private": true
}
